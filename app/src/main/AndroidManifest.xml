<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iyika.root.goart">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UpdateActivity" android:label="Post" />
        <activity android:name=".Display" android:label="My Posts" />
        <activity android:name=".Choose" android:label="Home" />
        <activity android:name=".Categories" android:label="Categories" />
        <activity android:name=".Paintings" android:label="Paintings" />
        <activity android:name=".Drawings" android:label="Drawings/Sketches" />
        <activity android:name=".Handicrafts" android:label="HandiCrafts"/>
        <activity android:name=".Digital" android:label="Digital Works"/>
        <activity android:name=".Fashion" android:label="Fashion/Clothing"/>
        <activity android:name=".Options"></activity>
    </application>

</manifest>